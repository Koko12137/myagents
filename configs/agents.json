{
    "debug": false, 
    "save_dir": "logs/", 
    "environment": {
        "type": "query", 
        "step_counters": [
            {
                "name": "base",
                "limit": 100
            }, 
            {
                "name": "token",
                "limit": 1000000
            }
        ],
        "agents": [
            {
                "type": "react",
                "llm": {
                    "provider": "openai",
                    "base_url": "http://localhost:8889/v1",
                    "model": "Qwen/Qwen3-14B-AWQ",
                    "temperature": 0.8, 
                    "api_key_field": "LOCAL_KEY", 
                    "extra_body": {
                        "enable_thinking": false
                    }
                }
            },
            {
                "type": "orchestrate",
                "llm": {
                    "provider": "openai",
                    "base_url": "http://localhost:8889/v1",
                    "model": "Qwen/Qwen3-14B-AWQ",
                    "temperature": 0.8, 
                    "api_key_field": "LOCAL_KEY", 
                    "extra_body": {
                        "enable_thinking": false
                    }
                }
            }, 
            {
                "type": "plan_and_exec",
                "llm": {
                    "provider": "openai",
                    "base_url": "http://localhost:8889/v1",
                    "model": "Qwen/Qwen3-14B-AWQ",
                    "temperature": 0.8, 
                    "api_key_field": "LOCAL_KEY", 
                    "extra_body": {
                        "enable_thinking": false
                    }
                }
            }
        ]
    }
}